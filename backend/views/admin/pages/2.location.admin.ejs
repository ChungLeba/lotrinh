                    <th>Đối tác</th>
                    <th>Nước</th>
                    <th>Tỉnh/thành</th>
                    <th>Quận/huyện</th>
                    <th>Phường/xã</th>
                    <th>Đường</th>
                    <th>Tên điểm</th>
                  </tr>
                </thead>
                <tbody>
                  <% for( let index = 0; index < partnerLocation.length; index++ ) { %>
                    <tr>
                      <td><%- index+1 %></td>
                      <td><%- moment(lotrinhLocation[index].timecreate).format("HH:mm:ss, DD/MM/YYYY") %></td>
                      <td><%- partnerLocation[index].by %></td>
                      <td><%- partnerLocation[index].nuoc %></td>
                      <td><%- partnerLocation[index].tinh %></td>
                      <td><%- partnerLocation[index].quan %></td>
                      <td><%- partnerLocation[index].phuong %></td>
                      <td><%- partnerLocation[index].duong %></td>
                      <td><%- partnerLocation[index].ten %></td>
                    </tr>
                  <% } %>
                 
                </tbody>
              </table>
            </div>
            <!-- /.card-body -->
