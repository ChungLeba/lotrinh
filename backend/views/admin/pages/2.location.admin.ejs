        </div>
      </div>
      <!-- Đia điểm lộ trình Build -->
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
              <h3 class="card-title"><span class="badge bg-danger">55</span> <strong> LỘ TRÌNH</strong> XÂY DỰNG</h3>

              <div class="card-tools">
                <div class="input-group input-group-sm" style="width: 150px;">
                  <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

                  <div class="input-group-append">
                    <button type="submit" class="btn btn-default">
                      <i class="fas fa-search"></i>
                    </button>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body table-responsive p-0" style="height: 300px;">
              <table class="table table-head-fixed text-nowrap">
                <thead>
                  <tr>
                    <th>No</th>
                    <th>Thời gian</th>
                    <th>Bởi</th>
                    <th>Nước</th>
                    <th>Tỉnh/thành</th>
                    <th>Quận/huyện</th>
                    <th>Phường/xã</th>
                    <th>Đường</th>
                    <th>Tên điểm</th>
                  </tr>
                </thead>
                <tbody>
                  <% for( let index = 0; index < lotrinhLocation.length; index++ ) { %>
                    <tr>
                      <td><%- index+1 %></td>
                      <td><%-moment(lotrinhLocation[index].timecreate).format("HH:mm:ss, DD/MM/YYYY") %></td>
                      <td><%- lotrinhLocation[index].by %></td>
                      <td><%- lotrinhLocation[index].nuoc %></td>
                      <td><%- lotrinhLocation[index].tinh %></td>
                      <td><%- lotrinhLocation[index].quan %></td>
                      <td><%- lotrinhLocation[index].phuong %></td>
                      <td><%- lotrinhLocation[index].duong %></td>
                      <td><%- lotrinhLocation[index].ten %></td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
        </div>
      </div>
      <!-- Đia điểm lộ trình Build -->
