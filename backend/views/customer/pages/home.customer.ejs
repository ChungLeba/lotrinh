<body class="sidebar-mini sidebar-collapse">
  <!-- Site wrapper -->
  <div class="container" itemscope itemtype ="http://lotrinh.vn">
    <%- include('../1.topmenu.customer.ejs'); %>
    <%- include('../2.seach.customer.ejs'); %>

        <!-- Phương tiện -->
        <div class="row bg-white">
          <h1 class="ml-2" style="font-size: large;">Có <%- data.total[6] %> tuyến đường phù hợp </h1>
        </div>
        
        <div class="row justify-content-center">
          <div class="col-md-9">
            <!-- Ads -->
            <%- include('../4.ads-auto.customer.ejs'); %>

                <% for( let index=0; index < data.kq.length; index++ ) { %>
                  <!-- Tuyến đường phù hợp -->
                    <div class="card card-outline card-success">
                      <div class="card-body">
                        <!-- Tuyến đường -->
                        <div class="row">
                          <div class="col-md-2">
                            <h2 class="text-center">
                              <%- data.kq[index].matuyen %>
                            </h2>
                            <% if (data.kq[index].loai=="1"){ %> 
                            <h6 class="text-center">Xe buýt</h6>
                            <% } else if (data.kq[index].loai=="2"){ %>
                              <h6 class="text-center">Xe khách</h6>
                            <% } else if (data.kq[index].loai=="3"){ %>
                              <h6 class="text-center">Xe taxi</h6>
                            <% } else if (data.kq[index].loai=="4"){ %>
                              <h6 class="text-center">Xe moto</h6>
                              <% } else if (data.kq[index].loai=="5"){ %>
                                <h6 class="text-center">Thuyền, tàu biển</h6>
                                <% } else if (data.kq[index].loai=="6"){ %>
                                  <h6 class="text-center">Phương tiện khác</h6>

                            <% } %>
                          </div>
                          <div class="col-md-10">
                            <a href="/lo-trinh/<%- data.kq[index].url_friendly %>">
                              <h3 class="text-center">
                                <%- data.kq[index].ten %>
                              </h3>
                              
                            </a>
                            <div class="row">
                              <div class="col">
                                <% if(data.kq[index].nccID){ %>
                                  <a href="/p/<%- data.kq[index].nccID._id %>">
                                    <h5 style="text-align: left;"><i class="fab fa-servicestack"> </i> <%- data.kq[index].nccID.tenncc %></h5>
                                  </a>
                                  
                                <% } %>  
                              </div>
                              <div class="col">
                                <a href="/user/<%- data.kq[index].partnerID._id %>">
                                  <h5 class="widget-user-username" style="text-align: right;"><i class="fas fa-user-circle"></i> <%- data.kq[index].partnerID.hoten %> 
                                  </h5>
                                </a>
                              </div>
                            </div>

                            
                          </div>
                          <div class="row">
                            
                          </div>
                        </div>

                        <div class="row">
                          <div class="col-sm-5 text-center">
                            <!-- Nơi đi -->
                            <h4><%- data.kq[index].chieudi[0].locationID.ten %></h4>
                              <p>
                                <%- data.kq[index].chieudi[0].locationID.duong %>, <%-
                                data.kq[index].chieudi[0].locationID.phuong %>, <%-
                                  data.kq[index].chieudi[0].locationID.quan %>, <%-
                                    data.kq[index].chieudi[0].locationID.tinh %>
                                    </p>
                          </div>
                          <div class="col-sm-2 text-center">
                            <i class="fas fa-angle-double-right"></i>
                          </div>
                          <div class="col-sm-5 text-center">
                            <!-- Nơi đến -->
                            <h4>
                              <%- data.kq[index].chieudi[data.kq[index].chieudi.length -1].locationID.ten %>
                            </h4>
                            <p>
                              <%- data.kq[index].chieudi[data.kq[index].chieudi.length -1].locationID.duong %>, <%-
                              data.kq[index].chieudi[data.kq[index].chieudi.length -1].locationID.phuong %>, <%-
                                data.kq[index].chieudi[data.kq[index].chieudi.length -1].locationID.quan %>, <%-
                                  data.kq[index].chieudi[data.kq[index].chieudi.length -1].locationID.tinh %>
                            </p>
                          </div>
                        </div>
                        <!-- Lộ trình -->
                        <div class="row">
                          <div class="col text-left">
                            <span class="info-box-text">
                              <i class="fas fa-route"></i> Đi qua: <strong>
                                <%- data.kq[index].chieudi.length %>
                              </strong>
                              địa điểm
                            </span>
                          </div>
                          <div class="col text-right">
                            <% if (data.kq[index].chieuve.length > 0) { %> 
                            <span class="info-box-text">
                              <i class="fas fa-sync"></i> Hai chiều
                            </span>
                            <% } else { %> 
                            <span class="info-box-text">
                              <i class="fas fa-long-arrow-alt-right"></i> Một chiều
                            </span>
                            <% } %> 
                          </div>
                        </div>
                        <% if (data.kq[index].chieudi[0].time[0]) { %> 
                        <div class="row mt-2">
                          <div class="col text-left">
                            <p><i class="nav-icon fas fa-calendar-alt"></i> Chiều đi: <strong><%- data.kq[index].chieudi[0].time.length%></strong> chuyến</p>

                          </div>
                          <div class="col text-right">
                            <p><i class="nav-icon fas fa-calendar-alt"></i> Chiều về: <strong><%- data.kq[index].chieuve[0].time.length%></strong> chuyến</p>
                          </div>
                        </div>
                        <% } %>
                      </div>
                    </div>
                  <% } %>
          </div>
          <div class="col-md-3">
            <!-- Lọc dịch vụ -->
            <%- include('../4A.filter_vehical.customer.ejs'); %>
            <!-- Lọc dịch vụ kết hợp tỉnh -->
            <%- include('../4B.filter_provin.customer.ejs'); %>
          </div>
          
        </div>
    <%- include('../3.footer.customer.ejs'); %>
  </div>
  <!-- ./wrapper -->

  <!-- jQuery -->
  <script src="../../admin/plugins/jquery/jquery.min.js"></script>
  <script>
    $(document).ready(function(){
      /* Gợi ý từ khóa */
      $("#input_homeseach").keyup(function(){
        let kytuseach = $("#input_homeseach").val()
        let last = kytuseach.charAt(kytuseach.length-1)
        if(kytuseach.length>=2&&last==" "){
          console.log("get..."+kytuseach)
          $.ajax({
              url:'/tim-kiem/?tukhoa='+kytuseach,
              type:'GET',
              dataType: 'json'
              })
              .then(data=>{
              console.log(data)
                $('#seach').empty()
                for (let index = 0; index < data.kq.length; index++) {
                  $('#seach').append(
                    `<option id='`+data.kq[index]._id+`' value='`+data.kq[index].ten+', '+data.kq[index].duong+', '+data.kq[index].phuong+', '+data.kq[index].quan+', '+data.kq[index].tinh+`' label='`+data.kq[index].byuserID.hoten+`'></option>`
                  )
                }
              })
              .catch(err=>{
              console.log(err)
            })
        }
      })
      $("#bt_seach").click(function() {
        let diadiem = $("#input_homeseach").val()
        window.location.href = '/tim-kiem/dia-diem/?tukhoa='+encodeURI(diadiem)
      })
    })
  </script>
  <!-- Bootstrap 4 -->
  <script src="../../admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
  <!-- AdminLTE App -->
  <script src="../../admin/dist/js/adminlte.min.js"></script>
