        </div>
        <!-- Ads -->
        <%- include('../4.ads-auto.customer.ejs'); %>
      <div class="col-md-12">
        <h1 class="fw-light text-center text-lg-start mt-4 mb-0" style="font-weight: lighter;">Các lịch khởi hành</h1>
      </div>
    
    </br>
      <!-- Lịch khởi hành -->
      <% if(data.cactuyen.length !== 0){ %> 
      <div class="row">
        <div class="col-md-12">
          <% for( let index = 0; index < data.cactuyen.length; index++ ) { %>
          
            <div class="card card-light collapsed-card">
            
              <div class="card-header">
                
                <a href="#">
                  <h3 class="card-title">
                    
                      
                      <button type="button" class="btn" style="font-size: larger;" data-card-widget="collapse">
                        <% if (data.cactuyen[index].loai=="1"){ %> 
                          <span class="badge bg-warning">Xe bus</span>
                          <% } else if (data.cactuyen[index].loai=="2"){ %>
                            <span class="badge bg-warning">Xe khách</span>
                          <% } else if (data.cactuyen[index].loai=="3"){ %>
                            <span class="badge bg-warning">Xe taxi</span>
                          <% } else if (data.cactuyen[index].loai=="4"){ %>
                            <span class="badge bg-warning">Xe moto</span>
                            <% } else if (data.cactuyen[index].loai=="5"){ %>
                              <span class="badge bg-warning">Thuyền, tàu biển</span>
                              <% } else if (data.cactuyen[index].loai=="6"){ %>
                                <span class="badge bg-warning">Phương tiện khác</span>
                          <% } %>
                        <%- data.cactuyen[index].ten %>
                      </button>
                     </h3>
                </a>
                  <div class="card-tools">
                    <span class="badge bg-danger"><%- data.cactuyen[index].matuyen %></span>
                    <span class="badge bg-light"><i class="far fa-eye"></i> <%- data.cactuyen[index].totalviews %></span>
                  </div>
              </div>
              <div class="card-body table-responsive p-0" style="height: auto;">
                
                  <table class="table table-head-fixed text-wrap">
                      <thead>
                        <tr>
                            <h4 class="text-center mt-2">Chiều đi</h4>
                        </tr>
                        <tr>
                          <div class="row ml-3 mr-3">
                            
                            
                            <div class="col text-center">
                              <p>Từ</p>
                              <a href="/dia-diem/<%- data.cactuyen[index].chieudi[0].locationID._id %>">
                                <h5 style="font-weight: bold;"><%- data.cactuyen[index].chieudi[0].locationID.ten %></h5>
                              </a>
                              <p><%- data.cactuyen[index].chieudi[0].locationID.duong %>, <%- data.cactuyen[index].chieudi[0].locationID.phuong %>, <%- data.cactuyen[index].chieudi[0].locationID.quan %>, <%- data.cactuyen[index].chieudi[0].locationID.tinh %></p>
                            </div>
                            <div class="col text-center">
                            </br>
                              <i class="fas fa-angle-double-right"></i>
                            </div>
                            <div class="col text-center">
                              <p>Đến</p>
                              <a href="/dia-diem/<%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID._id %>">
                                <h5 style="font-weight: bold;"><%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID.ten %></h5>
                              </a>
                              <p><%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID.duong %>, <%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID.phuong %>, <%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID.quan %>, <%- data.cactuyen[index].chieudi[data.cactuyen[index].chieudi.length - 1].locationID.tinh %></p>
                            
                            </div>
                          </div>
                        </tr>
                    <% if (data.cactuyen[index].chieudi[0].time.length!==0) { %> 
                      <tr>
                          <th>#</th>
                          <th>Mã chuyến</th>
                          <th>Thời gian khởi hành</th>
                          <th>Ngày khởi hành</th>
                          <th><i class="fas fa-route"></i></th>
                      </tr>
                      </thead>
                      <tbody id="tbody">
                          
                          <% for( let t= 0; t < data.cactuyen[index].chieudi[0].time.length; t++ ) { %>
                            <tr>
                              <td><%- t+1 %> </td>
                              <td><span class="badge bg-light"><%- data.cactuyen[index].chieudi[0].time[t].tripCode %></span></td>
                              <td><%- data.cactuyen[index].chieudi[0].time[t].time %></td>
                              <% if(data.cactuyen[index].chieudi[0].time[t].frequency == "one"){ %> 
                                <td>Hàng ngày</td>
                              <% } else { %> 
                                <td><%- data.cactuyen[index].chieudi[0].time[t].date %></td>
                                <% } %>
                              <td>
                                <a href="/lo-trinh/lich-trinh/chieu-di/<%- data.cactuyen[index]._id %>/chuyen/<%- data.cactuyen[index].chieudi[0].time[t].tripCode %>">
                                  <i class="fas fa-chevron-right"></i>
                                </a>
                              </td>
                          </tr>
                          <% } %> 
                      <!-- Loader -->
                      </tbody>
                    <% } else { %>
                      <p class="ml-3 text-center" style="font-style: italic;">Chưa có lịch khởi hành</p>
                    <% } %> 
                  </table>
                  <!-- Chiều về -->
                  <% if(data.cactuyen[index].chieuve.length!==0){ %>

                  <table class="table table-head-fixed text-wrap">
                    <thead>
                      <tr>
                          <h4 class="text-center mt-2">Chiều về</h4>
                      </tr>
                      <tr>
                        <div class="row ml-3 mr-3">
                          
                          
                          <div class="col text-center">
                            <p>Từ</p>
                            <a href="/dia-diem/<%- data.cactuyen[index].chieuve[0].locationID._id %>">
                              <h5 style="font-weight: bold;"><%- data.cactuyen[index].chieuve[0].locationID.ten %></h5>
                            </a>
                            <p><%- data.cactuyen[index].chieuve[0].locationID.duong %>, <%- data.cactuyen[index].chieuve[0].locationID.phuong %>, <%- data.cactuyen[index].chieuve[0].locationID.quan %>, <%- data.cactuyen[index].chieuve[0].locationID.tinh %></p>
                          </div>
                          <div class="col text-center">
                          </br>
                            <i class="fas fa-angle-double-right"></i>
                          </div>
                          <div class="col text-center">
                            <p>Đến</p>
                            <a href="/dia-diem/<%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID._id %>">
                              <h5 style="font-weight: bold;"><%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID.ten %></h5>
                            </a>
                            <p><%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID.duong %>, <%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID.phuong %>, <%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID.quan %>, <%- data.cactuyen[index].chieuve[data.cactuyen[index].chieuve.length - 1].locationID.tinh %></p>
                          
                          </div>
                        </div>
                      </tr>
                  <% if (data.cactuyen[index].chieuve[0].time.length!==0) { %> 
                    <tr>
                        <th>#</th>
                        <th>Mã chuyến</th>
                        <th>Thời gian khởi hành</th>
                        <th>Ngày khởi hành</th>
                        <th><i class="fas fa-route"></i></th>
                    </tr>
                    </thead>
                    <tbody id="tbody">
                        
                        <% for( let t= 0; t < data.cactuyen[index].chieuve[0].time.length; t++ ) { %>
                          <tr>
                            <td><%- t+1 %> </td>
                            <td><span class="badge bg-light"><%- data.cactuyen[index].chieuve[0].time[t].tripCode %></span></td>
                            <td><%- data.cactuyen[index].chieuve[0].time[t].time %></td>
                            <% if(data.cactuyen[index].chieuve[0].time[t].frequency == "one"){ %> 
                              <td>Hàng ngày</td>
                            <% } else { %> 
                              <td><%- data.cactuyen[index].chieuve[0].time[t].date %></td>
                              <% } %>
                            <td>
                              <a href="/lo-trinh/lich-trinh/chieu-di/<%- data.cactuyen[index]._id %>/chuyen/<%- data.cactuyen[index].chieuve[0].time[t].tripCode %>">
                                <i class="fas fa-chevron-right"></i>
                              </a>
                            </td>
                        </tr>
                        <% } %> 
                    <!-- Loader -->
                    </tbody>
                  <% } else { %>
                    <p class="ml-3 text-center" style="font-style: italic;">Chưa có lịch khởi hành</p>
                  <% } %> 
                </table>
                <% } %>
                  </div>
                  <!-- /.card-body -->
                  <div class="card-footer">
              </div>
            </div>
          <% } %>

              
        </div>
      </div>
      
      <% }%>
  </div>
  <!-- ./wrapper -->
<!-- Ads -->
<%- include('../3.ads.customer.ejs'); %>
